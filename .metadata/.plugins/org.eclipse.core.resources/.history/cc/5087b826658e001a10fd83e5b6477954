import java.util.ArrayList;
import java.util.List;

public class ListaTreino {

	public static void main(String[] args) {
		List<String> lista = new ArrayList<>();
		
		lista.add("Joao");
		lista.add("Vinicius");
		lista.add("Kioto");

		
		for(String x : lista) {
			System.out.println(x);
		}
	}

}

package entities;

public class Rooms {
	private String name;
	private String email;
	private int quarto;
	
	public Rooms(String name, String email, int quarto) {
		this.name = name;
		this.email = email;
		this.quarto = quarto;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getEmail() {
		return email;
	}
}

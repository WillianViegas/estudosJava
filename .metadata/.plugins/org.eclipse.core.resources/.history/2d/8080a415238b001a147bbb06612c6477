package entities;

public class Func {

	private Integer id;
	private String nome;
	private Double salario;
	

public Func(int id, String nome, double salario) {
	this.id = id;
	this.nome = nome;
	this.salario = salario;
}


public int getId() {
	return id;
}

public void setId() {
	this.id = id;
}

public String getNome() {
	return nome;
}

public void setNome() {
	this.nome = nome;
}

public double getSalario() {
	return salario;
}

public void setSalario(double salario) {
	this.salario = salario;
}
}
